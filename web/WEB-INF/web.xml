<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
    <display-name>Archetype Created Web Application</display-name>
    <welcome-file-list>
        <welcome-file>logs</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>ListLogs</servlet-name>
        <servlet-class>controller.ListLogsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListLogs</servlet-name>
        <url-pattern>/logs</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>LogOperation</servlet-name>
        <servlet-class>controller.LogOperationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LogOperation</servlet-name>
        <url-pattern>/manage</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ViewLogs</servlet-name>
        <servlet-class>controller.ViewLogsServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ViewLogs</servlet-name>
        <url-pattern>/open/*</url-pattern>
    </servlet-mapping>
</web-app>